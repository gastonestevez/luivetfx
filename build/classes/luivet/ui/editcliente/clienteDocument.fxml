<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXDrawer?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane id="rootPane" prefHeight="550.0" prefWidth="500.0" styleClass="custom-pane" stylesheets="@../res/dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="luivet.ui.editcliente.ClienteDocumentController">
   <children>
      <JFXDrawer defaultDrawerSize="150.0" resizableOnDrag="true" styleClass="jfx-drawer">
         <children>
            <AnchorPane id="rootAnchorPane" prefHeight="625.0" prefWidth="780.0">
               <children>
                  <Label fx:id="tituloLabel" layoutX="14.0" layoutY="14.0" text="Agregar cliente" textFill="#e2e2e2">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
                  <Label layoutX="14.0" layoutY="87.0" text="General" textFill="#e2e2e2">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label layoutX="14.0" layoutY="190.0" text="Ubicación" textFill="#e2e2e2">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label layoutX="14.0" layoutY="298.0" text="Comunicación" textFill="#e2e2e2">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <JFXTextField fx:id="nombreField" layoutX="14.0" layoutY="122.0" promptText="Nombre y Apellido" />
                  <JFXDatePicker fx:id="dateClientField" layoutX="256.0" layoutY="123.0" promptText="Fecha de inscripción" />
                  <JFXTextField fx:id="domicilioField" layoutX="14.0" layoutY="225.0" promptText="Domicilio" />
                  <JFXTextField fx:id="postalField" layoutX="256.0" layoutY="225.0" promptText="Codigo postal" />
                  <JFXTextField fx:id="telefonoField" layoutX="14.0" layoutY="333.0" promptText="Telefono" />
                  <JFXTextField fx:id="celularField" layoutX="246.0" layoutY="333.0" promptText="Celular" />
                  <JFXTextField fx:id="mailField" layoutX="14.0" layoutY="389.0" promptText="Email" />
                 
                  <Button fx:id="okButton" layoutX="14.0" layoutY="488.0" mnemonicParsing="false" onAction="#okButtonHandle" prefHeight="39.0" prefWidth="216.0">
                     <graphic>
                        <FontAwesomeIconView glyphName="CHECK" />
                     </graphic>
                  </Button>
                  <Button fx:id="cancelButon" layoutX="270.0" layoutY="488.0" mnemonicParsing="false" onAction="#cancelButtonHandle" prefHeight="39.0" prefWidth="216.0">
                     <graphic>
                        <FontAwesomeIconView glyphName="CLOSE" />
                     </graphic>
                  </Button>
               </children>
            </AnchorPane>
         </children></JFXDrawer> 
   </children>
</StackPane>
